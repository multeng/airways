{
  "name": "airways",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --open",
    "build": "ng build",
    "proxy": "ng serve --proxy-config proxy.conf.json",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "ng lint",
    "lint:fix": "prettier --write src && ng lint --fix",
    "deploy": "ng build --base-href=https://multeng.github.io/airways/ && npx angular-cli-ghpages --no-silent --dir=dist/airways"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged --relative",
      "pre-push": "lint-staged --relative"
    }
  },
  "lint-staged": {
    "src/**/*.{js,ts,html,css,scss}": [
      "prettier --write && eslint"
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.2.0",
    "@angular/cdk": "^15.0.0",
    "@angular/common": "^15.2.0",
    "@angular/compiler": "^15.2.0",
    "@angular/core": "^15.2.0",
    "@angular/forms": "^15.2.0",
    "@angular/platform-browser": "^15.2.0",
    "@angular/platform-browser-dynamic": "^15.2.0",
    "@angular/router": "^15.2.0",
    "@ngrx/effects": "^15.4.0",
    "@ngrx/store": "^15.4.0",
    "@ngrx/store-devtools": "^15.4.0",
    "@taiga-ui/addon-commerce": "^3.25.0",
    "@taiga-ui/addon-doc": "^3.25.0",
    "@taiga-ui/addon-mobile": "^3.25.0",
    "@taiga-ui/addon-table": "^3.25.0",
    "@taiga-ui/addon-tablebars": "^3.25.0",
    "@taiga-ui/cdk": "^3.25.0",
    "@taiga-ui/core": "^3.25.0",
    "@taiga-ui/icons": "^3.25.0",
    "@taiga-ui/kit": "^3.25.0",
    "@taiga-ui/styles": "^3.27.0",
    "@tinkoff/ng-dompurify": "^4.0.0",
    "angular": "^1.8.3",
    "angular-cli-ghpages": "^1.0.5",
    "dompurify": "^3.0.2",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "^0.13.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.2.2",
    "@angular-eslint/builder": "15.2.1",
    "@angular-eslint/eslint-plugin": "15.2.1",
    "@angular-eslint/eslint-plugin-template": "15.2.1",
    "@angular-eslint/schematics": "15.2.1",
    "@angular-eslint/template-parser": "15.2.1",
    "@angular/cli": "~15.2.2",
    "@angular/compiler-cli": "^15.2.0",
    "@ngrx/eslint-plugin": "^15.4.0",
    "@ngrx/schematics": "^15.4.0",
    "@types/dompurify": "^3.0.2",
    "@types/jasmine": "^4.3.0",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "eslint": "^8.33.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jasmine": "^4.1.3",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "jasmine-core": "^4.6.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "^3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "lint-staged": "^13.2.1",
    "prettier": "^2.8.7",
    "typescript": "^4.9.5"
  }
}
